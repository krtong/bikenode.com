# Discord Bot Configuration
# Replace these with your own values from Discord Developer Portal
# IMPORTANT: Never commit actual tokens to version control!

# Discord Bot Credentials
APP_ID=your_app_id_here
PUBLIC_KEY=your_public_key_here
DISCORD_BOT_TOKEN=your_bot_token_here

# Optional: Custom bot command prefix (default is "!bike ")
# BOT_PREFIX=!bike 

# API Connection
BIKENODE_API_KEY=your_api_key_here

# Database connection (if applicable)
DATABASE_URL=postgres://username:password@localhost:5432/bikenode

# Additional settings
DEBUG=false
