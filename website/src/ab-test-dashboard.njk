<script>
// Simulate real-time updates
document.addEventListener('DOMContentLoaded', function() {
    // Update metrics periodically
    setInterval(() => {
        // Update total users
        const totalUsers = document.querySelector('.metric-value');
        const current = parseInt(totalUsers.textContent.replace(',', ''));
        totalUsers.textContent = (current + Math.floor(Math.random() * 5)).toLocaleString();
    }, 5000);
    
    // Handle chart control clicks
    document.querySelectorAll('.chart-button').forEach(button => {
        button.addEventListener('click', function() {
            this.parentElement.querySelectorAll('.chart-button').forEach(b => {
                b.classList.remove('active');
            });
            this.classList.add('active');
        });
    });
});
</script>